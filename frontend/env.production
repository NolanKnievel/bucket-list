# Frontend Production Environment Configuration Template
# Copy this file to .env.local and fill in your production values

# =============================================================================
# REQUIRED VARIABLES (Production)
# =============================================================================

# Supabase Configuration
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key

# API Configuration
VITE_API_URL=https://api.yourdomain.com
VITE_WS_URL=wss://api.yourdomain.com

# =============================================================================
# PRODUCTION OPTIMIZED VARIABLES
# =============================================================================

# Application Configuration
VITE_APP_NAME=Collaborative Bucket List
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Analytics and Monitoring
VITE_ANALYTICS_ID=your-production-analytics-id
VITE_SENTRY_DSN=your-production-sentry-dsn

# Feature Flags (production settings)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEBUG_MODE=false

# =============================================================================
# PERFORMANCE SETTINGS
# =============================================================================

# Cache settings
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=3600

# =============================================================================
# SECURITY SETTINGS
# =============================================================================

# Content Security Policy
VITE_CSP_ENABLED=true

# =============================================================================
# MONITORING SETTINGS
# =============================================================================

# Performance monitoring
VITE_PERFORMANCE_MONITORING=true
VITE_ERROR_BOUNDARY_ENABLED=true 